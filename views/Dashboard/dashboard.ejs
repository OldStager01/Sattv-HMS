      <h4 class="mb-2 mt-5 font-medium">Total Clients</h4>
      <h3 class="mb-2 text-title-md font-bold text-black dark:text-white">
        <%= totalUsers %>
      </h3>
    </div>
    <!-- Total Clients End -->
  </div>
  <!-- ===== Data Stats End ===== -->

  <div class="mt-7.5 grid grid-cols-12 gap-4 md:gap-6 2xl:gap-7.5">
    <!-- ===== Leads Report Start ===== -->
    <div class="col-span-12">
      <div
        class="rounded-sm border border-stroke bg-white shadow-default dark:border-strokedark dark:bg-boxdark"
      >
        <div class="p-4 md:p-6 xl:p-7.5">
          <h2 class="text-title-sm2 font-bold text-black dark:text-white">
            Upcoming Clients
          </h2>
        </div>

        <div
          class="border-b border-stroke px-4 pb-5 dark:border-strokedark md:px-6 xl:px-7.5"
        >
          <!-- Header Row start -->
          <div class="flex items-center gap-3">
            <div class="w-4/12 xl:w-3/12">
              <span class="font-medium">Name</span>
            </div>
            <div class="w-6/12 2xsm:w-5/12 md:w-2/12">
              <span class="font-medium">Contact</span>
            </div>
            <div class="hidden md:w-3/12 xl:block xl:w-3/12">
              <span class="font-medium">Email</span>
            </div>
            <div class="w-4/12 md:w-3/12 xl:w-2/12">
              <span class="font-medium">Time</span>
            </div>
            <div class="hidden md:w-3/12 xl:block xl:w-2/12">
              <span class="font-medium">Payment Status</span>
            </div>
            <div
              class="hidden text-center 2xsm:block 2xsm:w-2/12 md:w-1/12 xl:w-1/12"
            >
              <span class="font-medium">View Profile</span>
            </div>
          </div>
        </div>
        <!-- Header Row end -->
        <!-- Table Data start -->
        <div class="p-4 md:p-6 xl:p-7.5">
          <div class="flex flex-col gap-7">
            <% if(users?.length === 0 || users == null) { %>
            <div class="text-center">No Appointments Scheduled for Today</div>
            <% }else{ users.forEach((user) => { %>
            <div class="flex items-center gap-3">
              <div class="w-4/12 xl:w-3/12">
                <!-- !Name -->
                <span class="font-medium"
                  ><%= user.firstName%> <%= user.lastName%></span
                >
              </div>
              <div class="w-6/12 2xsm:w-5/12 md:w-2/12">
                <!-- !Phone -->
                <span class="font-medium"><%= user.contact%></span>
              </div>
              <div class="hidden md:w-3/12 xl:block xl:w-3/12">
                <!-- !Email -->
                <span class="hidden font-medium xl:block"
                  ><%= user.email%></span
                >
              </div>
              <div class="w-4/12 md:w-3/12 xl:w-2/12">
                <!-- !Time -->
                <span class="font-medium appointmentTime"></span>
              </div>
              <div class="hidden md:w-3/12 xl:block xl:w-2/12">
                <!-- !Payment Status -->
                <span class="hidden font-medium xl:block"
                  ><%= user.paymentStatus%></span
                >
              </div>
              <div class="hidden w-2/12 2xsm:block md:w-1/12">
                <a
                  href="/user/<%= user.id %>"
                  class="inline-flex items-center justify-center rounded-full bg-primary px-6 py-2 text-center font-medium text-white hover:bg-opacity-90 lg:px-6 xl:px-6"
                >
                  View
                </a>
              </div>
            </div>
            <% });} %>
          </div>
        </div>
        <!-- Table Data end -->
      </div>
    </div>
    <!-- ===== Leads Report End ===== -->
  </div>
</div>
<script>
  const userData = `<%= users && JSON.stringify(users).replace(/&quot;/g, '"') %>`;
</script>
<script defer type="module" src="/js/dashboard/dashboard.js"></script>
